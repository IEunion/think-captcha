{
    "name": "ieunion/think-captcha",
    "description": "captcha package for thinkphp",
    "authors": [
        {
            "name": "ieunion",
            "email": "info@ieunion.app"
        }
    ],
    "license": "Apache-2.0",
    "require": {
        "topthink/framework": "^6.0.0"
    },
    "autoload": {
        "psr-4": {
            "think\\captcha\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "think": {
            "services": [
                "think\\captcha\\CaptchaService"
            ],
            "config":{
                "captcha": "src/config.php"
            }            
        }
    }
}
